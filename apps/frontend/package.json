{
  "name": "frontend",
  "version": "13.0.0",
  "main": "dist/electron/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "echo Prebuild check skipped",
    "build": "npx ng build --base-href ./",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npx eslint src --fix",
    "electron:start": "npm run build && npx -y esbuild electron/main.ts electron/preload.ts --bundle --platform=node --format=cjs --outdir=dist/electron --external:electron --external:node:* && manual-electron\\electron.exe .",
    "electron:local": "npm run build && tsc -p electron/tsconfig.json && unset ELECTRON_RUN_AS_NODE && node node_modules/electron/cli.js .",
    "electron:make": "npm run build && npx -y esbuild electron/main.ts electron/preload.ts --bundle --platform=node --format=cjs --outdir=dist/electron --external:electron --external:node:* && electron-forge make"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.14",
    "@angular/common": "^18.2.14",
    "@angular/compiler": "^18.2.14",
    "@angular/core": "^18.2.14",
    "@angular/forms": "^18.2.14",
    "@angular/platform-browser": "^18.2.14",
    "@angular/platform-browser-dynamic": "^18.2.14",
    "@angular/router": "^18.2.14",
    "rxjs": "~7.8.0",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.21",
    "@angular-eslint/eslint-plugin": "^21.1.0",
    "@angular-eslint/eslint-plugin-template": "^21.1.0",
    "@angular-eslint/schematics": "^21.0.1",
    "@angular-eslint/template-parser": "^21.1.0",
    "@angular/cli": "^18.2.21",
    "@angular/compiler-cli": "^18.2.14",
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@eslint/js": "^9.39.2",
    "@types/electron": "^1.4.38",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^25.0.6",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "electron": "^39.2.7",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "~5.5.2",
    "typescript-eslint": "^8.52.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
